{
    "log": "debug",
    "prefix": "cidl0",
    "dataset": "cifar100",
    "memory_size": 2000,
    "memory_per_class": 20,
    "fixed_memory": false,
    "shuffle": true,
    "init_cls": 10,
    "increment": 10,
    "forget_cls": [[],
                   [0,1],
                   [10,11],
                   [20,21],
                   [30,31],
                   [40,41],
                   [50,51],
                   [60,61],
                   [70,71],
                   [80,81]],
    "model_name": "icarl-mu2",
    "convnet_type": "resnet32",
    "batch_size": 128,
    "init_epoch": 200,
    "init_lr": 0.1,
    "init_milestones": [60, 120, 170],
    "init_lr_decay": 0.1,
    "init_weight_decay": 0.0005,
    "epochs": 170,
    "lr": 0.1,
    "milestones": [80, 120],
    "lr_decay": 0.1,
    "weight_decay": 2e-4,
    "T": 2,
    "lambda_clf": 1.0,
    "lambda_kd": 1.0,
    "lambda_forg": 1.0,
    "replay_retain_batch_size": 15,
    "replay_forget_batch_size": 8,
    "replay_with_replacement": true,
    "grad_conflict": true,
    "grad_conflict_interval": 1,
    "grad_conflict_include_fc": false,
    "grad_conflict_granular": true,
    "log_dir": "grad_conf",
    "device": ["1"],
    "seed": [1993],
    "num_workers": 8,
    "phase_id": 9,
    "pra_num_samples_per_class": 5,
    "pra_alpha": 1,
    "proto_n": 5,
    "proto_metric": "sqeuclidean",
    "proto_l2norm": true,
    "proto_batch_size": 128   
}

